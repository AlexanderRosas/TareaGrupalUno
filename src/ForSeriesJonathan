public class ForSeriesJonathan {
    public void imprimirJ(){
        System.out.print("");
        for(int i = 0; i <10; i++){
            System.out.println("J");
        }
    }
        System.out.print("");
        for(int i = 0; i <10; i++){
            System.out.println("JS");
        }
         System.out.print("");
        for(int i = 0; i <10; i++){
            System.out.println("S");
        }
}
